<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">

        <em:id>InspectElement@zbinlin</em:id>
        <em:version>0.0.7</em:version>
        <em:type>2</em:type>
        <em:name>Element Inspector</em:name>
        <em:optionsURL>chrome://InspectElement/content/options.xul</em:optionsURL>

        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.6</em:minVersion>
                <em:maxVersion>24.0a1</em:maxVersion>
            </Description>
        </em:targetApplication>

        <!-- Thunderbird -->
        <em:targetApplication>
            <Description>
                <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
                <em:minVersion>5.0</em:minVersion>
                <em:maxVersion>24.0a1</em:maxVersion>
            </Description>
        </em:targetApplication>

        <!-- SeaMonkey -->
        <em:targetApplication>
            <Description>
                <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
                <em:minVersion>1.0</em:minVersion>
                <em:maxVersion>2.27</em:maxVersion>
            </Description>
        </em:targetApplication>

        <em:bootstrap>true</em:bootstrap>
        <!--
        <em:unpack>true</em:unpack>
        -->
        
        <em:localized>
            <Description>
                <em:locale>zh-CN</em:locale>
                <em:name>Element Inspector</em:name>
                <em:description>使用 shift + 右键，在 DOM Inspector 中查找并定位到相应的元素节点</em:description>
                <em:creator>zbinlin</em:creator>
                <em:homepageURL>http://mozcp.com/</em:homepageURL>
                <em:developer>zbinlin</em:developer>
                <em:translator>zbinlin</em:translator>
            </Description>
        </em:localized>

        <em:localized>
            <Description>
                <em:locale>en-US</em:locale>
                <em:name>Element Inspector</em:name>
                <em:description>In the DOM Inspector,quickly navigate to the any elements</em:description>
                <em:creator>zbinlin</em:creator>
                <em:homepageURL>http://mozcp.com/</em:homepageURL>
                <em:developer>zbinlin</em:developer>
                <em:translator>zbinlin</em:translator>
            </Description>
        </em:localized>

        <em:requires>
            <Description>
                <!-- Require DOM Inspector --> 
                <em:id>inspector@mozilla.org</em:id>
                <em:minVersion>1.0</em:minVersion>
                <em:maxVersion>3.0</em:maxVersion>
            </Description>
        </em:requires>

    </Description>
</RDF>
